{
  "name": "home-expenses-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl true --host local.home-expenses.com --port 8443",
    "start-local": "ng serve -c local --ssl true --host local.home-expenses.com --port 8444",
    "start-railway": "ng serve -c dev --ssl true --host local.home-expenses.com --port 8445",
    "build-local": "ng build -c local",
    "build-dev": "ng build -c dev",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test-original": "ng test --no-watch",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "ng lint --fix",
    "deploy": "ng deploy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.2",
    "@angular/cdk": "^17.3.2",
    "@angular/common": "^17.3.2",
    "@angular/compiler": "^17.3.2",
    "@angular/core": "^17.3.2",
    "@angular/forms": "^17.3.2",
    "@angular/material": "^17.3.2",
    "@angular/platform-browser": "^17.3.2",
    "@angular/platform-browser-dynamic": "^17.3.2",
    "@angular/router": "^17.3.2",
    "@ng-bootstrap/ng-bootstrap": "^14.1.1",
    "@ngrx/effects": "^17.1.1",
    "@ngrx/router-store": "^17.1.1",
    "@ngrx/store": "^17.1.1",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "buffer": "^6.0.3",
    "ng-let": "^17.0.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.2",
    "@angular-eslint/builder": "17.3.0",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/schematics": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "^17.3.2",
    "@angular/compiler-cli": "^17.3.2",
    "@angular/localize": "^17.3.2",
    "@ngrx/eslint-plugin": "^17.1.1",
    "@ngrx/store-devtools": "^17.1.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "angular-cli-ghpages": "^1.0.7",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.0.3",
    "libsodium-wrappers": "^0.7.13",
    "prettier": "^2.8.3",
    "prettier-eslint": "^15.0.1",
    "typescript": "~5.3.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup.jest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/docs/"
    ]
  }
}
